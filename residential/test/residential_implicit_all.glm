clock {
	timezone EST+5EDT;
	starttime '2010-01-01 0:00:00';
	stoptime '2010-01-02 0:00:00';
}

module tape;
module residential;

#set profiler=1
#set iteration_limit=10

object house:..100 {
	floor_area random.normal(1500,300);
}
object collector {
	group "class=house";
	property avg(air_temperature);
	file "house.csv";
	interval 3600;
	limit 24;
};
