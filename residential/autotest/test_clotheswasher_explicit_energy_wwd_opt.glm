module residential;
module assert;
module tape;

object house {
	heating_setpoint 5;
	cooling_setpoint 300;
	air_temperature 70;
	outdoor_temperature 73;
	object clotheswasher {
		motor_power 1000W;
		shape "type: pulsed; schedule: washing; energy: 1 kWh; count: 4; power: 1 kW; stdev: 0.15 kW";
		
		//Weekend winter test
		//WARNING: This test may be off by 1 hour. Please adjust accordingly.
		object complex_assert {
			target "energy";
			within .01;
			in '2001-01-01 23:59:59 PDT';
			value 4;
		};
		object recorder {
			property enduse_load,energy;
			file "test_dryer_external_power.csv";
			interval 1800;
			limit 53;
		};
		
	};
	
}



