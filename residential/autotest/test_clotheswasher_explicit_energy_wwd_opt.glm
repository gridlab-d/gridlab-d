// Test for clotheswasher energy usage during a weekend in the winter
// TEST WILL FAIL UNTIL CLOTHESWASHER IS PROPERLY LINKED UP TO ENDUSE_LOAD; MAY NEED TO BE EDITED AFTERWARDS
// 
// payroll: d3y332

#set suppress_repeat_messages=1

clock {
	timezone PST+8PDT;
	starttime '2001-01-01 00:00:00 PDT';
	stoptime '2001-01-02 01:00:00 PDT';
}


schedule washing {

		weekday-summer { 
