object house {
	system_mode OFF;
	heating_system_type NONE;
	cooling_system_type NONE;
	auxiliary_system_type NONE;
	number_of_stories 1;
	object lights {
		type INCANDESCENT;
		power_factor 0.97;
		heatgain_fraction 0.9;
		breaker_amps 30;
		impedance_fraction 0.5;
		current_fraction 0.1;
		power_fraction 0.4;
		shape "type: analog; schedule: light_load; power: 0.76 kW";
	};
	object recorder {
		property panel.energy,internal_gain;
		file test_lights_exp.csv;
		interval 3600;
		limit 8760;
	};
	object complex_assert {
		target panel.energy;
		once ONCE_TRUE;
		in '2001-12-31 23:00:00';
		value 3531.39+885.051j;
		within 3.531;
	};
}