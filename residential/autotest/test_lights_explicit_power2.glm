module residential;
module assert;
module tape;

object house {
	heating_setpoint 5;
	cooling_setpoint 300;
	air_temperature 70;
	outdoor_temperature 73;
	object lights {
		type HID;
		installed_power 760 W;
		shape "type: analog; schedule: lighting; power: .76 kW";
		
		//Weekend Summer test
		//WARNING: This test may be off by 1 hour. Please adjust accordingly.
		object complex_assert {
			target power;
			within .01;
			object player {
				property value;
				file "../test_lights_explicit_power2.player";
				loop 1;
			};
		};
		
	};
	object recorder{
		property power;
		file "test_lights_explicit_power2.csv";
		interval 3600;
		limit 24;
	};
	
	
	
}



