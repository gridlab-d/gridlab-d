// $Id: test_lights_implicit_power2.glm 4738 2014-07-03 00:55:39Z dchassin $
// Test: Single lights object inside of a house with an implicit schedue for the Weekend Summer.
// ID: D3P706


#set suppress_repeat_messages=0

clock {
	timezone PST+8PDT;
	starttime '2001-07-01 00:00:00 PDT';
	stoptime '2001-07-02 00:00:00 PDT';
}

module residential {
	implicit_enduses LIGHTS;
}
module assert;
module tape;

object house {
	heating_setpoint 5;
	cooling_setpoint 300;
	air_temperature 70;
	outdoor_temperature 73;
	object recorder{
		property panel;
		file "test_lights_implicit_power.csv";
		interval 3600;
		limit 10000;
	};
	object complex_assert {
		target panel.power;
		within 1;
		object player {
			property value;
			file "test_lights_explicit_power2.player";
			loop 1;
		};
	};
}



