object house {
	name House;
	heating_setpoint 5;
	cooling_setpoint 300;
	air_temperature 70;
	outdoor_temperature 73;
	object plugload {
		installed_power .36;
		heatgain_fraction .9;
		shape "type: analog; schedule: plugs; power: .36 kW";
		object complex_assert {
			once ONCE_TRUE;
			target "power";
			object player {
				property value;
				file "../test_plugload_enduse_load_swy.player";
			};
			within 1e-6;
		};
		object complex_assert {
			once ONCE_TRUE;
			target "energy";
			object player {
				property value;
				file "../test_plugload_energy_swy.player";
			};
			within 1e-6;
		};
		object double_assert {
			once ONCE_TRUE;
			target "heatgain";
			object player {
				property value;
				file "../test_plugload_heatgain_swy.player";
			};
			within 1e-3;
		};
		object recorder {
			property power,energy,heatgain;
			file "test_plugload_explicit_swy.csv";
			interval 3600;
			limit 30;
		};
	};
}
