// Test that waterheater is updating temperature and height of hot water column correctly over a day with scheduled demand.

#set profiler=1;
//#set pauseatexit=1;
clock {
     //timezone PST+8PDT;
	 timezone PST8;
     starttime '2009-01-01 00:00:00';
     stoptime '2009-01-02 00:00:00';
}

module tape;
module climate;
module residential;
module assert;
module powerflow {
     solver_method NR;
     NR_iteration_limit 50;
};
schedule daily_use {
     * 0 * * * 0.00;
     * 1 * * * 0.00;
     * 2 * * * 0.00;
    0-9 3 * * * 0.14;
    10-19 3 * * * 0.15;
    20-29 3 * * * 0.50;
    30-39 3 * * * 0.97;
