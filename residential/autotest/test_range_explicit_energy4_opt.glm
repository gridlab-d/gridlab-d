module residential;
module assert;
module tape;

object house {
	heating_setpoint 5;
	cooling_setpoint 300;
	air_temperature 70;
	outdoor_temperature 73;
	object range {
		installed_power 500 W;
		shape "type:pulsed; schedule: cooking; energy: 1.0 kWh; power: 0.5 kW; count: 1; stdev: 95 W";
		
		//Weekend Winter test
		//WARNING: This test may be off by 1 hour. Please adjust accordingly.
		object complex_assert {
			target "energy";
			within .01;
			in '2001-03-25 23:59:59 PST';
			value 1;
		};
		object recorder{
			property enduse_load,energy;
			file "test_range_explicit_energy4.csv";
			interval 1800;
			limit 48;
		};
		
	};
	
	
	
	
}



