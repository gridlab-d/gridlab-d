// Tape test
// - shaper generates a 24 hour saw tooth
// - player generates an 8 hour saw tooth
// - recorder collect 8760 hours

#set include="../../core"
// #include "../../core/rt/msvc_debugger.glm"
module tape;

clock {
	timezone EST+5EDT;
	starttime '2000-01-01 00:00:00 EST';
}

class test {
	double saw24;
	double saw8;
	intrinsic create(object parent)
	{
		return SUCCESS;
	};
};

object test {
	name Test;
	object shaper {
		file "test.shaper";
		group "class=test";
		property "saw24";
	};
	object player {
		file "test.player";
		property "saw8";
	};
	object recorder {
		file "test.csv";
		property "saw8,saw24";
		limit 8760;
	};
};


