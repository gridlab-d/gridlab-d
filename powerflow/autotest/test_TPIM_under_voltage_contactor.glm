object motor {
    name TPIM;
    parent LoadNode;
    phases ABCN;
    frequency_measure_type PLL;
    base_power 35000.0;
    motor_trip true;
    reconnect_time 100 s;
    trip_time 0.5;
    nominal_voltage 2401.777;
    uv_relay_install UNINSTALLED;
    contactor_open_Vmin 0.6;
    contactor_close_Vmax 0.8;
    object enum_assert {
    target motor_status;
    flags DELTAMODE;
        object player {
            property value;
            file "../TPIM_under_voltage_contactor_status.player";
            loop 1;
            flags DELTAMODE;
       };
   };
}


//Base system objects -- nodes
object meter {
	phases ABCN;
	name SwingNode;
	bustype SWING;
	nominal_voltage 2401.777;
}

object meter {
	phases ABCN;
	name TopSwitchNode;
	nominal_voltage 2401.777;
}

object meter {
	phases ABCN;
	name BottomSwitchNode;
	nominal_voltage 2401.777;
}

object meter {
	phases ABCN;
	name LoadNode;
	frequency_measure_type PLL;
	nominal_voltage 2401.777;
}

//Base system objects -- lines
object overhead_line {
	phases ABCN;
	from SwingNode;
	to TopSwitchNode;
	configuration lc303;
	length 25.0 ft;
}

object overhead_line {
	phases ABCN;
	from BottomSwitchNode;
	to LoadNode;
	configuration lc303;
	length 150.0 ft;
}

//Base system objects - switch
object switch {
	phases ABC;
	name InRushSwitch;
	from TopSwitchNode;
	to BottomSwitchNode;
	status CLOSED;
}

