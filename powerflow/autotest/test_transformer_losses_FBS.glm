object meter {
	name node1;
	phases "ABCD";
	bustype SWING;
	voltage_A +7199.558+0.000j;
	voltage_B -3599.779-6235.000j;
	voltage_C -3599.779+6235.000j;
	nominal_voltage 7200;
}

object overhead_line {
	name ohl12;
	phases "ABC";
	from node1;
	to node2;
	length 2000;
	configuration lc300;
}

object node {
	name node2;
	phases "ABCD";
	nominal_voltage 7200;
}

object overhead_line {
	name ohl12del;
	phases "ABC";
	from node1;
	to node2del;
	length 2000;
	configuration lc300;
}

object node {
	name node2del;
	phases "ABCD";
	nominal_voltage 7200;
}

//DGwye
object meter {
	phases ABCD;
	name node2dy;
	nominal_voltage 7200.0;
	parent node2del;
}
	
object transformer {
	name xform23dy;
	phases "ABCN";
	from node2dy;
	to load4dy;
	configuration tconfigdy;
	object complex_assert {
		target power_losses;
		within 0.5;
		value 129706.5+80.5377d;
	};
}

object meter {
	name load4dy;
	phases "ABCN";
	nominal_voltage 2400;
}

object load {
	name load4dy_load;
	parent load4dy;
	phases "ABCN";
	constant_power_A +1800000.000+800000.000j;
	constant_power_B +180000.000+80000.000j;
	constant_power_C +18000.000+8000.000j;
	nominal_voltage 2400;
}

