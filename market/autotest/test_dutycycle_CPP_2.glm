object load {
	name load_4;
	phases "ABCN";
	name load4;
	voltage_A +2401.777+0.000j;
	voltage_B -1200.889-2080.000j;
	voltage_C -1200.889+2080.000j;
	constant_power_A +1000;
	constant_power_B +1000;
	constant_power_C +1000;
	nominal_voltage 2400;
}

object transformer_configuration {
		name AS_config;
		connect_type SINGLE_PHASE_CENTER_TAPPED;
		install_type POLETOP;
		powerA_rating 100;
		primary_voltage 2401.777;
		secondary_voltage 120.000;
		impedance 0.0006+0.000136j;

}

object transformer {
	phases AS;
	configuration AS_config;
	from load4;
	to triplex_1;
	nominal_voltage 2401.777;
	object recorder {
		file test_dutycycle_CPP.csv;
		interval 60;
		property power_out.real;
	};
	object complex_assert {
		operation REAL;
		target power_out;
		within 3;
		object player {
			property value;
			file ../dutycycle_CPP_player.player;
			loop 1;
		};
	};	
}

object triplex_node {
	name triplex_1;
	nominal_voltage 120;
	phases AS;
}

object triplex_line_conductor {
	name tplconfig1;
	resistance 0.97;
	geometric_mean_radius 0.0111;
};

object triplex_line_configuration {
     name TPLconfig;
     conductor_1 tplconfig1;
     conductor_2 tplconfig1;
     conductor_N tplconfig1;
     insulation_thickness 0.08;
     diameter 0.368;
}

object triplex_line {
	configuration TPLconfig;
	length 50;
	phases AS;
	from triplex_1;
	name tpl1;
	to triplex_2;	
}

object triplex_meter {
	name triplex_2;
	nominal_voltage 120;
	phases AS;
}

object house {
     parent triplex_2;
     groupid Residential;
     floor_area 1711;
     //Thermal integrity -> Single Family 5
     Rroof 34.06;
     Rwall 19.73;
     Rfloor 22.48;
     glazing_layers 2;
     glass_type 1;
     glazing_treatment 1;
     window_frame 2;
     Rdoors 3.23;
     airchange_per_hour 1.01;
     cooling_COP 2.5;
     air_temperature 71.71;
     mass_temperature 71.71;
     total_thermal_mass_per_floor_area 3.487;
     heating_system_type NONE;
     cooling_system_type NONE;
     cooling_setpoint 100;
     heating_setpoint 10;

     object ZIPload {
          // Pool Pump
           base_power 1.5;
		   duty_cycle 0.2;
		   phase 0.7;
		   period 6.2;
           heatgain_fraction 0.0;
           power_pf 1.000;
           current_pf 1.000;
           impedance_pf 1.000;
           impedance_fraction 0;
           current_fraction 0;
           power_fraction 1;
		   recovery_duty_cycle 0.55;
		   
		   object passive_controller {
				period 300;
				control_mode DUTYCYCLE;
				pool_pump_model true;
				observation_object STUB;
				observation_property next.P;
				state_property override;
				base_duty_cycle 0.2;
				setpoint duty_cycle;
				first_tier_hours 12;
				second_tier_hours 12;
				first_tier_price 1;
				second_tier_price 2;
				third_tier_price 10;
		   };

     };
}

