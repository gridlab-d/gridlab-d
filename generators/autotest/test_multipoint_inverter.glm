object climate {
     name "WA-Seattle";
     tmyfile "../WA-Yakima.tmy2";
	 //tmyfile "WA-Yakima.tmy2";
     interpolate QUADRATIC;
};

object triplex_meter {
	name trip_swing;
	bustype SWING;
	phases AS;
	nominal_voltage 120.0;
}

object triplex_meter {
	name trip_fixed_meter;
	parent trip_swing;
	phases AS;
	nominal_voltage 120.0;
}

object inverter {
	name trip_lat;
	phases AS;
	parent trip_fixed_meter;
	generator_mode CONSTANT_PF;
    generator_status ONLINE;
    inverter_type PWM;
    power_factor 1.0;
	inverter_manufacturer XANTREX;
	use_multipoint_efficiency true;
		 object complex_assert {
				target "VA_Out";
					object player {
						property value;
						file "../MPINV_OUT1.player";
						//file "MPINV_OUT1.player";
					};
				within 0.01;
		};
		object recorder {	                	       
				file MPINV_OUT1.csv;
       	     	interval 3600;
       	     	property VA_Out;
	    };
}

object solar {
	name trip_lat_solar;
	phases AS;
	parent trip_lat;
	Rated_kVA 4.0 kVA;
	area 29.6296 m^2;
	latitude_angle_fix true;
	efficiency 0.135;
	orientation_azimuth 25.0;
	orientation FIXED_AXIS;
	object recorder {
		property "Insolation,VA_Out.real";
		file "fixed_array_lat_angle_fix.csv";
		interval 1800;
		};
}

