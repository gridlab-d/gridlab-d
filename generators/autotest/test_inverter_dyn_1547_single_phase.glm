//Tests single-phase versions of inverter_dyn
//Check for test that was failing due to indexing on single-phase

clock {
	timezone "PST+8PDT";
	starttime '2001-08-01 12:00:00 PDT';
	stoptime '2001-08-01 12:00:6 PDT';
}

#set suppress_repeat_messages=1
#set relax_naming_rules=1;
#set profiler=1
//#set pauseatexit=1
#set double_format=%+.12lg
#set complex_format=%+.12lg%+.12lg%c

module tape;
module assert;
module reliability {
	enable_subsecond_models true;
	maximum_event_length 1800000;	//Maximum length of events in seconds (manual events are excluded from this limit)
	report_event_log true;
}

module powerflow {
	lu_solver "KLU";
	enable_subsecond_models true;
	deltamode_timestep 1 ms;	//10 ms
	solver_method NR;
	all_powerflow_delta true;
	//enable_inrush TRUE;
};

module generators {
	enable_subsecond_models true;
	deltamode_timestep 1 ms;
}

object meter {
	name subtation_bus;
	phases "ABCN";
	bustype SWING;
	nominal_voltage 7200.0;
	object player {
		file ../data_7200v_1s_delta_trigger.player;
		property nominal_voltage;
		flags DELTAMODE;
	};
}

object overhead_line:  {
     phases "ABCN";
     name intermed_line;
     from subtation_bus;
     to feeder_head;
     length 100;
     configuration olcABCN;
}

object meter {
	name feeder_head;
	nominal_voltage 7200;
	phases ABCN;
}

object node {
	name nA;
	phases AN;
	nominal_voltage 7200;
}

object node {
	name nB;
	phases BN;
	nominal_voltage 7200;
}

object node {
	name nC;
	phases CN;
	nominal_voltage 7200;
}

object overhead_line:  {
     phases "AN";
     name lA;
     from feeder_head;
     to nA;
     length 12;
     configuration olcAN;
}

object overhead_line:  {
     phases "BN";
     name lB;
     from feeder_head;
     to nB;
     length 12;
     configuration olcBN;
}

object overhead_line:  {
     phases "CN";
     name lC;
     from feeder_head;
     to nC;
     length 12;
     configuration olcCN;
}


