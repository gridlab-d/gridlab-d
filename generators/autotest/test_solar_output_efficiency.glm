// Test power out of the inverter for default single-point efficiency and for multipoint efficiency model

#set profiler=1;

clock {
     //timezone PST+8PDT;
	 timezone PST8;
     starttime '2009-01-01 00:00:00';
     stoptime '2010-01-01 00:00:00';
}

module tape;
module climate;
module generators;
module assert;

module powerflow {
     solver_method NR;
     NR_iteration_limit 50;
};

object climate {
     name "WA-Yakima";
     tmyfile "../WA-Yakima.tmy2";
	 interpolate NONE;
};

object triplex_meter {
	name trip_swing;
	bustype SWING;
	phases AS;
	nominal_voltage 120.0;
}

object triplex_meter {
	name trip_fixed_meter;
	parent trip_swing;
	phases AS;
	nominal_voltage 120.0;
}

object inverter {
	name trip_inv1;
	phases AS;
	parent trip_fixed_meter;
	rated_power 25000;
	object complex_assert {
		target VA_Out;
		operation REAL;
		object player {
			property value;
			file "../inverter_VA_out_default_solar.player";
		};
		within 0.05;
	};
//	 object recorder {
//		 property "VA_In.real, VA_Out.real";
//		 file "inverter1_VA_in_VA_out.csv";
//		 interval 3600;
//	 };
}

//default solar object
object solar {
	name trip_solar1;
	parent trip_inv1;
	area 323;
	object double_assert {
		target Insolation;
		object player {
			property value;
			file "../default_solar_Insolation.player";
		};
		within 0.01;
	};
	object double_assert {
		target P_Out;
		object player {
			property value;
			file "../default_solar_VA_out.player";
		};
		within 0.05;
	};

}

object inverter {
	name trip_inv2;
	phases AS;
	parent trip_fixed_meter;
	inverter_type FOUR_QUADRANT;
	rated_power 4500.0;
	use_multipoint_efficiency TRUE;
	// The following CEC parameters are for NREL SAM multipoint efficency curve: SAM/Sandia Inverters/Fronius USA, LLC:IG4500-LV NEG 208V [CEC 2006]
	maximum_dc_power 4834.61;
	maximum_dc_voltage 277.888;
	minimum_dc_power 35.5073;
	c_0 -5.8408e-006;
	c_1 -2.3810e-005;
	c_2 -0.0012;
	c_3 -0.0015;
	object complex_assert {
		target VA_Out;
		operation REAL;
		object player {
			property value;
			file "../mltpt_efficiency_inverter_VA_out.player";
		};
		within 0.05;
	};
//	 object recorder {
//		 property "VA_In.real, VA_Out.real";
//		 file "inverter2_VA_in_VA_out.csv";
//		 interval 3600;
//	 };
}

//default solar object
object solar {
	name trip_solar2;
	parent trip_inv2;
	weather "WA-Yakima";
	rated_power 4.0 kW;
	area 29.6296 m^2;
	tilt_angle 45.0;
	efficiency 0.135;
	orientation_azimuth 180.0; //south
	orientation FIXED_AXIS;
	SOLAR_TILT_MODEL SOLPOS;
	SOLAR_POWER_MODEL FLATPLATE;
//	 object recorder {
//		 property "Insolation,VA_Out,shading_factor";
//		 file "deafult_array.csv";
//		 interval 3600;
//		 };
	object double_assert {
		target P_Out;
		object player {
			property value;
			file "../fixed_45_S_VA_out.player"; 
		}; 
		within 0.05;
	};
	object double_assert {
		target Insolation;
		object player {
			property value;
			file "../fixed_45_S_Insolation.player";
		};
		within 0.01;
	};

}

