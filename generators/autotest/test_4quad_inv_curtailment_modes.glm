object transformer_configuration {
	name trans_config_to_feeder;
	connect_type WYE_WYE;
	install_type PADMOUNT;
	primary_voltage 33000;
	secondary_voltage 2401.777;
	power_rating 5 MVA;
	impedance 0.00033+0.0022j;
}
object transformer_configuration {
		name inv_transformer;
		connect_type SINGLE_PHASE_CENTER_TAPPED;
		install_type POLETOP;
		shunt_impedance 10000+10000j;
		primary_voltage 2401.777;
		secondary_voltage 120;
		powerB_rating 110 kVA;
		impedance 0.00033+0.0022j;
	};

object node {
	name network_node;
	bustype SWING;
	nominal_voltage 33000;
	phases ABCN;
}

object transformer {
	name substation_transformer;
	from network_node;
	to 645;
	phases ABCN;
	configuration trans_config_to_feeder;
}; 

object node {     
	name 645;     
	phases ABCN;     
	voltage_A 2401.7771;     
	voltage_B -1200.8886-2080.000j;     
	voltage_C -1200.8886+2080.000j;     
	nominal_voltage 2401.7771;
}

object meter {
	name p_priority_meter;
	parent 645;
	phases ABCN;
	voltage_A 2401.7771;     
	voltage_B -1200.8886-2080.000j;     
	voltage_C -1200.8886+2080.000j;     
	nominal_voltage 2401.7771;
	//object recorder {
	//	property measured_power;
	//	file p_priority_inv.csv;
	//	interval 3600;
	//};
	object complex_assert {
		target measured_power;
		object player {
			property value;
			file "../p_priority.player";
		};
		within "0.1";
	};
}

object inverter {
	name p_priority_inverter;
	parent p_priority_meter;
	inverter_type FOUR_QUADRANT;
	use_multipoint_efficiency FALSE;
	four_quadrant_control_mode CONSTANT_PQ;
	curtailment_priority_mode WATT;
	rated_power 1340;
	P_Out output_power*1;
	Q_Out output_var*1;
	V_In 266.0;
	I_In 15.1203;
}

object meter {
	name q_priority_meter;
	parent 645;
	phases ABCN;
	voltage_A 2401.7771;     
	voltage_B -1200.8886-2080.000j;     
	voltage_C -1200.8886+2080.000j;     
	nominal_voltage 2401.7771;
	//object recorder {
	//	property measured_power;
	//	file q_priority_inv.csv;
	//	interval 3600;
	//};
	object complex_assert {
		target measured_power;
		object player {
			property value;
			file "../q_priority.player";
		};
		within "0.1";
	};
}

object inverter {
	name q_priority_inverter;
	parent q_priority_meter;
	inverter_type FOUR_QUADRANT;
	use_multipoint_efficiency FALSE;
	four_quadrant_control_mode CONSTANT_PQ;
	curtailment_priority_mode VAR;
	rated_power 1340;
	P_Out output_power*1;
	Q_Out output_var*1;
	V_In 266.0;
	I_In 15.1203;
}  

object transformer {
	name CTTF_B_645;
	phases BS;
	from 645;
	to tn_B_645;
	configuration inv_transformer;
}

object triplex_meter { 
	name tn_B_645;
	phases BS;
	nominal_voltage 120;
	groupid House_Meter;
	// object recorder {
		// property measured_real_power,measured_reactive_power,power_12;
		// file const_pf_meter.csv;
		// interval 3600;
	// };
	object complex_assert {
		target measured_power;
		object player {
			property value;
			//file "constantPFmeterPower_static_efficiency.player";
			file "../constantPFmeterPower_static_efficiency.player";
		};
		within "0.1";
	};
	object inverter {
		name const_pf_inv;
		phases BS;
		inverter_type FOUR_QUADRANT;
		power_factor pf_sched*1;
		use_multipoint_efficiency FALSE;
		four_quadrant_control_mode CONSTANT_PF;
		V_In 266.0;
		I_In 15.1203;
		// object recorder {
			// property V_In,I_In,VA_In,VA_Out;
			// file const_pf_inv.csv;
			// interval 3600;
			// limit 25;
		// };
	};
}
