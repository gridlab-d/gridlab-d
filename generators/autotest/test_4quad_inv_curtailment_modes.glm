module powerflow;
module generators;
module tape;
module assert;
module powerflow{
	solver_method NR;
	default_maximum_voltage_error 1e-9;
}

clock {
	timezone PST+8PDT;
	timestamp '2010-06-01 0:00:00';
	stoptime '2010-06-02 1:00:00';
