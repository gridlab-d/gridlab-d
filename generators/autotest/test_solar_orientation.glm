object triplex_meter {
	name trip_swing;
	bustype SWING;
	phases AS;
	nominal_voltage 120.0;
}

object triplex_meter {
	name trip_fixed_meter;
	parent trip_swing;
	phases AS;
	nominal_voltage 120.0;
}

object inverter {
	name trip_shad_inv;
	phases AS;
	parent trip_fixed_meter;
	rated_power 25000;
	//generator_mode CONSTANT_PF; //DEFAULT
	//generator_status ONLINE; //DEFAULT
	//inverter_type PWM; //DEFAULT
	// object recorder {
		// property "VA_In.real, VA_Out.real";
		// file "inverter_VA_in_VA_out.csv";
		// interval 3600;
	// };
}

// solar panels use the following defaults:
//    control mode SUPPLY_DRIVEN
//    status ONLINE
//    panel type SINGLE_CRYSTAL_SILICON

object solar {
	name trip_shad_solarS;
	phases AS;
	parent trip_shad_inv;
	weather "WA-Yakima";
	Rated_kVA 4.0 kVA;
	area 29.6296 m^2;
	tilt_angle 45.0;
	efficiency 0.135;
	orientation_azimuth 180.0; //equator-facing (South)
	orientation FIXED_AXIS;
	SOLAR_TILT_MODEL SOLPOS;
	SOLAR_POWER_MODEL FLATPLATE;
	object complex_assert {
		target VA_Out;
		operation REAL;
		object player {
			property value;
			file "../fixed_45_S_VA_out.player"; 
		}; 
		within 0.01;
	};
	object double_assert {
		target Insolation;
		object player {
			property value;
			file "../fixed_45_S_Insolation.player";
		};
		within 0.01;
	};
	// object recorder {
		// property "Insolation,VA_Out.real,shading_factor";
		// file "fixed_array_45_S.csv";
		// interval 3600;
	// };
}

