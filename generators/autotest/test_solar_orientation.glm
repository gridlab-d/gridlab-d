// Verify solar model algorithms are correct by validating power output for solar objects with different orientations (S,N,E,W)
// GridLab-D output has been validated against SAM output. See "Incorporation of NREL Solar Advisor Model Photovoltaic Capabilities with GridLAB-D" (Tuffner et al. 2012). 

#set profiler=1;
//#set pauseatexit=1;

clock {
     //timezone PST+8PDT;
	 timezone PST8;
     starttime '2009-01-01 00:00:00';
     stoptime '2010-01-01 00:00:00';
}

module tape;
module climate;
module generators;
module assert;

module powerflow {
     solver_method NR;
     NR_iteration_limit 50;
};

object climate {
     name "WA-Yakima";
     tmyfile "../WA-Yakima.tmy2";
	 interpolate NONE;
};

