// $Id$
// Basic deltamode test
// Does parented devices
// This autotest is mainly to check meter accumulations of diesel_dg
// but also duplicates some of the aspects of the parent/child tests

#set suppress_repeat_messages=0
//#set profiler=1
#set dateformat=US
#define rotor_convergence=0.0001
// #set verbose=1

#set double_format=%+.12lg
#set complex_format=%+.12lg%+.12lg%c

//Deltamode declarations - global values
#set deltamode_timestep=100000000		//100 ms
#set deltamode_maximumtime=60000000000	//1 minute
#set deltamode_iteration_limit=10		//Iteration limit

clock {
	timezone "PST+8PDT";
	starttime '2001-01-01 00:00:00 PST';
	stoptime '2001-01-01 00:00:30 PST';
}

module assert;
module tape;
module powerflow {
	enable_subsecond_models true;
	all_powerflow_delta true;
	deltamode_timestep 10.0 ms;	//10 ms
	solver_method NR;
};
module generators {
	enable_subsecond_models TRUE;
	deltamode_timestep 10000.0 us;	//10 ms
}

//Reference line type
object line_configuration {
	name OHL_config;
	z11 0.3465+1.0179j;	//Ohms/mile
	z12 0.1560+0.5017j;
	z13 0.1580+0.4236j;
	z21 0.1560+0.5017j;
	z22 0.3375+1.0478j;
	z23 0.1535+0.3849j;
	z31 0.1580+0.4236j;
	z32 0.1535+0.3849j;
	z33 0.3414+1.0348j;
}

//Power system
object meter {
	phases ABC;
	name BUS_1;
	nominal_voltage 8660.254;
	flags DELTAMODE;
	// object recorder {
		// file bus_1_output_recorder.csv;
		// property voltage_A.real,voltage_A.imag,voltage_B.real,voltage_B.imag,voltage_C.real,voltage_C.imag;
		// flags DELTAMODE;
		// //interval -1;
		// interval 1;
	// };
	// object recorder {
		// property "measured_power.real,measured_power.imag,measured_power_A.real,measured_power_A.imag,measured_power_B.real,measured_power_B.imag,measured_power_C.real,measured_power_C.imag";
		// interval 1;
		// flags DELTAMODE;
		// file "bus_1_rec_power.csv";
	// };
	
	//Meter flow should basically net to zero, just due to how this is being posted (generator absorbs all flow)
	object complex_assert {
		flags DELTAMODE;
		target measured_power;
		within 1;
		value 0.0;
	};	
	object complex_assert {
		flags DELTAMODE;
		target measured_power_A;
		within 1;
		value 0.0;
	};	
	object complex_assert {
		flags DELTAMODE;
		target measured_power_B;
		within 1;
		value 0.0;
	};	
	object complex_assert {
		flags DELTAMODE;
		target measured_power_C;
		within 1;
		value 0.0;
	};	
}

object meter {
	phases ABC;
	name BUS_1_p;
	parent BUS_1;
	nominal_voltage 8660.254;
	flags DELTAMODE;
	// object recorder {
		// property "measured_power.real,measured_power.imag,measured_power_A.real,measured_power_A.imag,measured_power_B.real,measured_power_B.imag,measured_power_C.real,measured_power_C.imag";
		// interval 1;
