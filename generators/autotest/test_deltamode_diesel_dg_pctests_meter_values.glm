		// file "bus_1_p_rec_power.csv";
	// };
	// object recorder {
		// property "measured_power";
		// interval 1;
		// flags DELTAMODE;
		// file "data_diesel_meter_accum_bus_1_p_total_power.csv";
	// };
	// object recorder {
		// property "measured_power_A";
		// interval 1;
		// flags DELTAMODE;
		// file "data_diesel_meter_accum_bus_1_p_phase_A_power.csv";
	// };
	// object recorder {
		// property "measured_power_B";
		// interval 1;
		// flags DELTAMODE;
		// file "data_diesel_meter_accum_bus_1_p_phase_B_power.csv";
	// };
	// object recorder {
		// property "measured_power_C";
		// interval 1;
		// flags DELTAMODE;
		// file "data_diesel_meter_accum_bus_1_p_phase_C_power.csv";
	// };
	object complex_assert {
		flags DELTAMODE;
		target measured_power;
		within 1;
		object player {
			flags DELTAMODE;
			property value;
			file "../data_diesel_meter_accum_bus_1_p_total_power.csv";
		};
	};	
	object complex_assert {
		flags DELTAMODE;
		target measured_power_A;
		within 1;
		object player {
			flags DELTAMODE;
			property value;
			file "../data_diesel_meter_accum_bus_1_p_phase_A_power.csv";
		};
	};	
	object complex_assert {
		flags DELTAMODE;
		target measured_power_B;
		within 1;
		object player {
			flags DELTAMODE;
			property value;
			file "../data_diesel_meter_accum_bus_1_p_phase_B_power.csv";
		};
	};	
	object complex_assert {
		flags DELTAMODE;
		target measured_power_C;
		within 1;
		object player {
			flags DELTAMODE;
			property value;
			file "../data_diesel_meter_accum_bus_1_p_phase_C_power.csv";
		};
	};	
}

