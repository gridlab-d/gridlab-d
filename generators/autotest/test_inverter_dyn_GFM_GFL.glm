	control_mode GRID_FOLLOWING;
	grid_following_mode POSITIVE_SEQUENCE;	
	frequency_watt true;
	volt_var true;

	rated_power 200 kVA;  
	Pref 60000; 
	Qref 10000;
			
	flags DELTAMODE;

    Rfilter 0.01;
	Xfilter 0.10;
	
	kpPLL  50;
	kiPLL  900;
	

	kpc  0.05;
	kic  5;
