// Test the use of alternate property value syntax (A) ? (B) : (C);
//

#set tmp=../test_opt_alternate_syntax
#setenv GRIDLABD=../../../core

class test {
	enumeration {NO=0, YES=1} value;
    intrinsic create(object parent)
	{
		return 1;
	};
}

object test:..100 {
	value (random.bernoulli(0.8)) ? YES : NO;
}

