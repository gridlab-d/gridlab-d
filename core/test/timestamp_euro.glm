#set dateformat=EURO

class test {
	timestamp a;
	intrinsic create (object parent) {
		return 1;
	};
	intrinsic init (object parent) {
		gl_output("a = %s", gl_strftime(a));
		return 1;
	};
}

object test {
	a '24/03/2002 17:32:28';
}